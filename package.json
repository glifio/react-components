{
  "name": "@glif/react-components",
  "version": "2.0.0-alpha.157",
  "description": "Shared react components for @glif projects",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/glifio/react-components.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "eslint --ext .jsx,.js,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .jsx,.js,.ts,.tsx ./src --fix",
    "test:jest": "jest --coverage",
    "test-watch": "jest --watch",
    "clean": "rimraf dist/* && mkdirp dist",
    "codegen": "graphql-codegen --config codegen.yml && eslint ./src/generated/graphql.tsx --fix",
    "build": "run-s clean codegen build:*",
    "build:babel": "babel src --out-dir dist --extensions \".ts,.tsx,.js,.jsx\" --source-maps --ignore 'src/**/*.stories.jsx','src/**/*.test.jsx','src/**/*.test.js','src/**/*.test.tsx','src/**/*.test.ts'",
    "build:types": "tsc --emitDeclarationOnly",
    "watch": "npm run clean && run-p watch:*",
    "watch:babel": "npm run codegen && npm run build:babel -- --watch",
    "start": "npm run watch",
    "storybook": "start-storybook -s ./src/public -p 6006",
    "deploy:storybook": "storybook-to-ghpages",
    "prepare": "npm run build",
    "jest-dev": "jest --watch",
    "update-snapshots": "npm run test:jest -- -u"
  },
  "contributors": [
    "Bret Comnes <bcomnes@gmail.com> (https://bret.io/)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.17.12",
    "@graphql-codegen/cli": "2.11.2",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.3",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^28.1.1",
    "@types/react": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-jest": "^28.1.1",
    "eslint": "^8.4.1",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-styled-components": "7.0.8",
    "mkdirp": "^1.0.4",
    "next": "^12.2.0",
    "npm-run-all2": "^6.0.1",
    "prettier": "^2.1.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.2.1",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@chainsafe/filsnap-adapter": "^2.1.1",
    "@chainsafe/filsnap-types": "^2.1.2",
    "@glif/base-css": "^0.0.33",
    "@glif/filecoin-actor-utils": "^2.0.6",
    "@glif/filecoin-address": "^2.0.1",
    "@glif/filecoin-message": "^2.0.1",
    "@glif/filecoin-number": "^2.0.5",
    "@glif/filecoin-rpc-client": "^2.0.1",
    "@glif/filecoin-wallet-provider": "^2.0.5",
    "@glif/logger": "^2.0.1",
    "@ipld/dag-cbor": "^7.0.2",
    "@ledgerhq/hw-transport": "^6.27.1",
    "@zondax/filecoin-signing-tools": "^1.0.0",
    "axios": "^0.27.2",
    "base32-decode": "^1.0.0",
    "bignumber.js": "^9.0.1",
    "bip39": "^3.0.4",
    "dayjs": "^1.9.6",
    "graphql": "^16.2.0",
    "graphql-ws": "^5.9.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "multiformats": "^9.6.5",
    "prop-types": "^15.7.2",
    "subscriptions-transport-ws": "^0.11.0",
    "swr": "^1.0.1"
  },
  "peerDependencies": {
    "next": ">=12.0.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "styled-components": ">=5.2.1"
  },
  "files": [
    "dist"
  ]
}
